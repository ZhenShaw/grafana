{
  "version": "2.0",
  "envId": "{{env.ENV_ID}}",
  "framework": {
    "name": "redis",
    "plugins": {
      "client": {
        "use": "@cloudbase/framework-plugin-container",
        "inputs": {
          "mode": "low-cost",
          "serviceName": "redis",
          "servicePath": "/redis",
          "localPath": "./",
          "uploadType": "image",
          "imageInfo": {
            "imageUrl": "ccr.ccs.tencentyun.com/tcb_public/grafana_grafana:7.2.0"
          },
          "containerPort": 3000,
          "envVariables": {},
          "volumeMounts": {
            "/data": "redis-cfs"
          }
        }
      }
    },
    "requirement": {
      "addons": [
        {
          "type": "CFS",
          "name": "redis-cfs"
        }
      ]
    }
  },
  "functionRoot": "./functions",
  "functions": [],
  "region": "ap-shanghai"
}
